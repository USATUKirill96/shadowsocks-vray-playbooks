{
    "server":"0.0.0.0",
    "server_port":9000,
    "password":"{{ proxy_password }}",
    "timeout":300,
    "method":"chacha20-ietf-poly1305",
    "fast_open":true,
    "nameserver":"1.1.1.1",
    "mode":"tcp_only",
    "plugin":"v2ray-plugin",
    "plugin_opts":"server;tls;host={{ host }};cert=/data/certs/live/{{ host }}/fullchain.pem;key=/data/certs/live/{{ host }}/privkey.pem;"
}